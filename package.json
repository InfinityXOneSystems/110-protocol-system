{
  "name": "110-protocol-system",
  "version": "1.0.0",
  "description": "Implements 110% Protocol - exceeding expectations, continuous enhancement, boundary pushing",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "tap --reporter=spec --timeout=30 'tests/**/*.test.ts'",
    "test:coverage": "tap --reporter=spec --timeout=30 'tests/**/*.test.ts'",
    "test:ci": "tap --reporter=tap --timeout=30 'tests/**/*.test.ts'",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts' 'tests/**/*.ts'",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "clean": "rm -rf dist coverage .tap",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "110-protocol",
    "continuous-improvement",
    "autonomous-system",
    "self-healing",
    "self-learning",
    "tap-protocol"
  ],
  "author": "InfinityXOne Systems",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "tap": "^18.6.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  }
}
